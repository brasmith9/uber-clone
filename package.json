{
  "name": "uber-clone",
  "version": "1.0.0",
  "description": "An uber backend clone",
  "main": "index.ts",
  "author": "Isaac Amankwaah Anane",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc --project './tsconfig.json'",
    "start:dev": "cross-env tsc --project './tsconfig.json' --watch & NODE_ENV=development nodemon -r esm ./dist",
    "start": "npx tsc --project './tsconfig.json' && cross-env NODE_ENV=production node -r esm ./dist",
    "test": "npx tsc --project './tsconfig.json' && cross-env UBER_CLONE_NODE_ENV=test  nyc --reporter=lcov --reporter=text --reporter=text-summary mocha -c -r esm '{,!(node_modules)/**}dist/**/*.test.js' --timeout 12000 --exit",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/**.ts'"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.24",
    "app-root-path": "^3.0.0",
